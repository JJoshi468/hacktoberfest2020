#include <iostream>
#include <algorithm> 
using namespace std;

	void findPairk(int arr[], int n, int k){
	 	sort(arr, arr+n); 
	 	int  i, j;
	 	for(i = 0;i< n-1;i++){
	 		for( j=i+1;j<n;j++){
		 	
	        if(arr[i] + arr[j] == k){
	        		
		 cout<<arr[i]<<","<<arr[j]<<"\n";	      
			
			}
			 
		 }
		
	}
	
	
}
	        

int main() {
	int n, arr[100];
	cin>>n; //size of array
	for(int i= 0;i<n; i++) 
		cin>>arr[i];
	int k;
	cout<<"k: ";
	cin>>k;
	cout<<"Pairs with sum k are: ";
	findPairk(arr,n,k);
	
	return 0;
}
